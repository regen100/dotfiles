[core]
	pager = less -R -F -X
[push]
	default = simple
[credential]
	helper = cache
[merge]
	conflictStyle = diff3
[alias]
	co = checkout
	pushf = push --force-with-lease
	m = !git co master && git fetch && git rb && git su
	rc = rebase --continue
	rb = pull --rebase origin master
	su = submodule update --init --recursive
